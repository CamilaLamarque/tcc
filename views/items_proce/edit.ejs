<h1>Editar Item do Procedimento</h1>
<form action="/items_proces/<%= items_proce.cod %>?_method=PUT" method="POST">
    <div class="form-group">
        <label for="procedimentos">Procedimento</label>
        <input type="text" class="form-control" id="procedimentos" name="procedimentos" value="<%= items_proce.procedimentos %>" required placeholder="Insira o procedimento">
    </div>
    <div class="form-group">
        <label for="produtos">Produto</label>
        <input type="text" class="form-control" id="produtos" name="produtos" value="<%= items_proce.produtos %>" required placeholder="Insira o produto">
    </div>
    <div class="form-group">
        <label for="quantidade">Quantidade</label>
        <input type="number" class="form-control" id="quantidade" name="quantidade" value="<%= items_proce.quantidade %>" required min="1" placeholder="Quantidade mínima é 1">
    </div>
    <button type="submit" class="btn btn-warning">Salvar</button>
</form>
<a href="/items_proces" class="btn btn-primary">Voltar para a Lista</a>
